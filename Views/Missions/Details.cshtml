        <div class="mb-3">
            功能開啟/關閉 :
        </div>
        <div class="mb-3">
            @if (Model.course.IsAddMetacognition)
            {
                if (Model.mis.IsGoalSetting)
                {
                    <button type="button" class="btn btn-success" id="is_Goalsetting">目標設置開啟</button>
                }
                else
                {
                    <button type="button" class="btn btn-secondary" id="is_Goalsetting">目標設置關閉</button>
                }
            }
            else
            {
                <button type="button" class="btn btn-secondary" id="is_Goalsetting" hidden>目標設置關閉</button>
            }

            @if (Model.mis.IsDiscuss)
            {
                <button type="button" class="btn btn-success" id="is_Discuss">討論區開啟</button>
            }
            else
            {
                <button type="button" class="btn btn-secondary" id="is_Discuss">討論區關閉</button>
            }
            @if (Model.mis.IsDrawing)
            {
                <button type="button" class="btn btn-success" id="is_Drawing">流程圖開啟</button>
            }
            else
            {
                <button type="button" class="btn btn-secondary" id="is_Drawing">流程圖關閉</button>
            }
            @if (Model.mis.IsCoding)
            {
                <button type="button" class="btn btn-success" id="is_Coding">程式碼開啟</button>
            }
            else
            {
                <button type="button" class="btn btn-secondary" id="is_Coding">程式碼關閉</button>
            }
            @if (Model.course.IsAddPeerAssessmemt)
            {
                if (Model.mis.IsAssess)
                {
                    <button type="button" class="btn btn-success" id="is_Assess">自評與互評開啟</button>
                }
                else
                {
                    <button type="button" class="btn btn-secondary" id="is_Assess">自評與互評關閉</button>
                }

                if (Model.mis.IsGReflect)
                {
                    <button type="button" class="btn btn-success" id="is_GReflect">組間互評開啟</button>
                }
                else
                {
                    <button type="button" class="btn btn-secondary" id="is_GReflect">組間互評關閉</button>
                }
            }
            else
            {
                <button type="button" class="btn btn-secondary" hidden id="is_Assess">自評與互評關閉</button>
                <button type="button" class="btn btn-secondary" hidden id="is_GReflect">組間互評關閉</button>

            }

            @if (Model.course.IsAddMetacognition)
            {
                if (Model.mis.IsReflect)
                {
                    <button type="button" class="btn btn-success" id="is_Reflect">自我反思開啟</button>
                }
                else
                {
                    <button type="button" class="btn btn-secondary" id="is_Reflect">自我反思關閉</button>
                }
            }
            else
            {
                <button type="button" class="btn btn-secondary" id="is_Reflect" hidden>自我反思</button>
            }

            @if (Model.mis.Is_Journey)
            {
                if (Model.course.IsAddPeerAssessmemt)
                {
                    <button type="button" class="btn btn-success" id="is_ejourney">學習表現(互評版)開啟</button>
                }
                else
                {
                    <button type="button" class="btn btn-success" id="is_journey">學習表現開啟</button>
                }
            }
            else
            {
                if (Model.course.IsAddPeerAssessmemt)
                {
                    <button type="button" class="btn btn-secondary" id="is_ejourney">學習表現(互評版)關閉</button>
                }
                else
                {
                    <button type="button" class="btn btn-secondary" id="is_journey">學習表現關閉</button>
                }
            }
        </div>
    </div>
</div>

@section Scripts
