                    if (response_data['Status'] == 200) {
                        $("#is_journey").removeClass();

                        if (response_data['sw'] == false) {
                            $("#is_journey").text("學習表現關閉");
                            $("#is_journey").addClass("btn btn-secondary");
                        } else {
                            $("#is_journey").text("學習表現開啟");   //狀態為開啟
                            $("#is_journey").addClass("btn btn-success");
                        }
                    }
                }
            });
        });
